<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:c="urn:import:com.google.gwt.user.cellview.client">


    <g:VerticalPanel>
        <g:Label>Transactions</g:Label>
        <g:HorizontalPanel>
            <g:Button ui:field="addButton">Add transaction</g:Button>
            <g:Button ui:field="updateButton">Update transaction</g:Button>
            <g:Button ui:field="deleteButton">Delete transaction</g:Button>
        </g:HorizontalPanel>
        <c:CellTable ui:field="cellTable"/>
    </g:VerticalPanel>



</ui:UiBinder>