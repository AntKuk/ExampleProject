<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <g:DialogBox >
        <g:HTMLPanel>
            <g:Label>Company</g:Label>
            <g:ListBox ui:field="companyName"/>
            <g:Label>Bank</g:Label>
            <g:ListBox ui:field="bankName"/>
            <g:Label>Correspondence account</g:Label>
            <g:TextBox ui:field="account"/>
            <g:Button ui:field="ok">Ok</g:Button>
            <g:Button ui:field="cancel">Cancel</g:Button>
        </g:HTMLPanel>
    </g:DialogBox>

</ui:UiBinder>