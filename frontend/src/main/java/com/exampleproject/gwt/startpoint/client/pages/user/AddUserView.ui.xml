<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <g:DialogBox >
        <g:HTMLPanel>
            <g:Label>Login</g:Label>
            <g:TextBox ui:field="loginTB"/>
            <g:Label>Password</g:Label>
            <g:TextBox ui:field="pwdTB"/>
            <g:Label>Admin?</g:Label>
            <g:CheckBox ui:field="isAdmin"/>
            <g:Button ui:field="ok">Ok</g:Button>
            <g:Button ui:field="cancel">Cancel</g:Button>
        </g:HTMLPanel>
    </g:DialogBox>

</ui:UiBinder>